{
  "name": "schema-dot-org-types",
  "version": "0.1.0",
  "description": "Classes with flow types to encapsulate schema.org markup attributes.",
  "main": "index.js",
  "scripts": {
    "build": "npm run test && flow && rm -rf ./lib && babel src -d lib && babel tools -d bin && rollup -c -i src/index.js && flow-copy-source src lib",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register",
    "transform-schema": "node bin/transform-schema.js"
  },
  "files": [
    "lib",
    "bin",
    "index.js",
    "index.module.js"
  ],
  "module": "index.module.js",
  "keywords": [
    "schema.org",
    "jsonld"
  ],
  "author": "John Bito (http://jwbito.ballardview.com/)",
  "license": "UPL-1.0",
  "devDependencies": {
    "JSONStream": "^1.3.1",
    "babel-core": "^6.24.1",
    "chai": "^3.5.0",
    "commander": "^2.9.0",
    "decamelize": "^1.2.0",
    "event-stream": "^3.3.4",
    "find-up": "latest",
    "flow-bin": "^0.46.0",
    "flow-copy-source": "^1.1.0",
    "load-json-file": "latest",
    "mocha": "^3.4.2",
    "moment": "^2.18.1",
    "mustache": "^2.3.0",
    "request": "^2.81.0",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "latest",
    "rollup-plugin-commonjs": "latest",
    "rollup-plugin-node-resolve": "latest",
    "schema.org": "^2.4.0",
    "sinon": "^2.3.1",
    "url-template": "^2.0.8"
  },
  "dependencies": {
    "object-assign": "^4.1.1"
  }
}
